    pipeline {
    agent any

    stages {
        stage('Chekout') {
            steps {
                git 'https://github.com/AshantaP/boxfuse-sample-java-war-hello.git'
            }
        }
        stage('Quality gate check') {
            steps {
                withSonarQubeEnv('sonarqube') {
                    sh 'mvn clean package sonar:sonar'
                }
            }
        }
    }
}
